/*[C_MM21-易] 算階乘*/
#include <iostream>
#include <algorithm>
#include <string.h>
using namespace std;

int main()
{
    int num;
    while(scanf("%d",&num)!=EOF)
    {
        long long int total=1;
        for(int i=1;i<=num;i++)
        {
            total *= i;
        }
        cout << total << endl;
    }
}
