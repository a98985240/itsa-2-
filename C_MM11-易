/*[C_MM11-易] 購票計算*/
#include <iostream>
#include <algorithm>
#include <string.h>
using namespace std;

int main()
{
    int num;
    cin >> num;
    int num_ten=0;
    int num_five=0;
    int num_one=0;
    num_ten=num/10;
    num %= 10;
    num_five=num/5;
    num%=5;
    num_one=num;
    cout <<"NT10="<<num_ten<<endl;
    cout <<"NT5="<<num_five<<endl;
    cout <<"NT1="<<num_one<<endl;
}
