/*[C_SO10-易] 選擇排序法*/
#include <iostream>
#include <algorithm>
#include <string.h>
using namespace std;

int main()
{
    int num;
    int array[40];
    cin >> num;
    for(int i=0;i<num;i++)
    {
        cin >> array[i];
    }
    sort(array,array+num);
    for(int i=0;i<num-1;i++)
    {
        cout << array[i] << ' ';
    }
    cout << array[num-1] << endl;
}
