/*[C_AR72-易] 電影票購買計算*/
#include <iostream>
#include <algorithm>
#include <string.h>
#include <cmath>
using namespace std;

int main()
{
    int num;
    cin >> num ;
    for(int i=0;i<num;i++)
    {
        int a,b;
        int total;
        cin >> a >> b;
        total=250*a+175*b;
        cout << total << endl ;
    }
}
